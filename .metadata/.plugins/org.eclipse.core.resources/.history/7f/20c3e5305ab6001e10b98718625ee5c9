package br.com.fiap.banco;

public class Teste {

	public static void main(String[] args) {
		
		Conta cc = new Conta();
		cc.saldo = 50.0;
		cc.agencia = 123;
		cc.numero =321;
		
		cc.depositar(1000);
		
		System.out.println("Saldo Conta Corrente: R$ " + cc.verificarSaldo());
		
		Conta cp = new Conta(111, 222, 1000) ;
		cp.retirar(50);
		
		System.out.println("Saldo Conta Poupan√ßa: R$ " + cp.verificarSaldo());

	}

}
